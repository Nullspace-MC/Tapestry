{
	"schema_version": 1,
	"quilt_loader": {
		"group": "net.nullspacemc",
		"id": "tapestry",
		"version": "${version}",
		"metadata": {
			"name": "Tapestry",
			"description": "A Fabric-Carpet-like mod for old Minecraft versions",
			"contributors": {
				"Pineapplecake": "Owner"
			},
			"contact": {
				"issues": "https://github.com/Nullspace-MC/Tapestry/issues",
				"sources": "https://github.com/Nullspace-MC/Tapestry"
			}
